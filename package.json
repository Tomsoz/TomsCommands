	"license": "GNU GPLv3",
	"devDependencies": {
		"@types/node": "^22.13.9",
		"discord.js": "^14.18.0",
		"drizzle-kit": "^0.30.5",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.40.0",
		"mysql2": "^3.13.0",
		"zod": "^3.24.2"
	}
}
