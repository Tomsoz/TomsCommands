	},
	"devDependencies": {
		"@types/node": "^22.13.9",
		"discord.js": "^14.19.3",
		"drizzle-kit": "^0.30.5",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2"
	}
