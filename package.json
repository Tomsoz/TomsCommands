{
	"name": "toms-discord-handlers",
	"version": "1.0.0",
	"description": "A collection of discord handlers for my discord bots",
	"main": "dist/index.js",
	"types": "@types/index.d.ts",
	"type": "module",
	"scripts": {
		"tsc": "tsc -w --outDir dist --rootDir src --skipLibCheck",
		"compile": "tsc && node index.js",
		"dev": "nodemon -e ts --exec \"npm run compile\""
	},
	"keywords": [
		"discord",
		"handlers",
